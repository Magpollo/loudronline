{
  "kind": "collectionType",
  "collectionName": "creators",
  "info": {
    "singularName": "creator",
    "pluralName": "creators",
    "displayName": "Creator",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "creatorId": {
      "type": "customField",
      "customField": "plugin::field-uuid.uuid"
    },
    "name": {
      "type": "string"
    },
    "bio": {
      "type": "text"
    },
    "profileImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "socialLinks": {
      "type": "json"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "creator"
    }
  }
}
